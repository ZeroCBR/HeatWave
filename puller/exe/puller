#!/usr/bin/env ruby

require 'puller'

source = { hostname: 'ftp2.bom.gov.au',
           filename: '/anon/gen/fwo/IDA00003.dat',
           user: 'ftp',
           passwd: '' }

pipeline = { getter: Puller::Getter,
             processor: Puller::Processor,
             marshaler: Marshal }

puts Puller.pull_from(source, pipeline)
